<section class="auth_section register">
    <div class="auth_container">
        <div class="cart">
            <div class="row">
                <form class="col s12" method="POST" action="/login">
                    <div class="row ">
                        <div class="input-field col s12">
                            <i class="material-icons prefix">email</i>
                            <input name="email" value="<%= messages.name %>" id="icon_email" type="email" class="validate">
                            <label for="icon_email">Email Address</label>
                        </div>
                        <div class="input-field col s12">
                            <i class="material-icons prefix">password</i>
                            <input name="password" id="icon_password" type="password" class="validate">
                            <label for="icon_password">Password</label>
                        </div>
                        <div class="input-field col s12">
                              <div class="auth_bun">
                                  <button type="submit" class="waves-effect waves-light btn"><i class="material-icons left">login</i>Login</button>
                              </div>
                              <p>Don't have an account? <a href="/register">Register</a></p>
                              <p> <a href="/login">Forgot password?</a></p>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>

</section>